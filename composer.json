{
    "name": "outmart/laravel-core",
    "description": "eCommerce Core to help you build your eCommerce based on laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ecommerce",
        "outmart",
        "customers",
        "baskets",
        "modules"
    ],
    "authors": [
        {
            "name": "Karim Mohamed (Komtcho)",
            "email": "komicho1996@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "Bidaea\\OutMart\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bidaea\\OutMart\\OutMartServiceProvider",
                "Bidaea\\OutMart\\Modules\\Catalogs\\CatalogsServiceProvider",
                "Bidaea\\OutMart\\Modules\\Customers\\CustomersServiceProvider",
                "Bidaea\\OutMart\\Modules\\Baskets\\BasketsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}